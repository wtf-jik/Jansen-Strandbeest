JANSEN

LINK_ANGLES[] = UNKNOWN;	% 
LINK_LENGTHS[] = KNOWN;		% 
CRANK_ANGLES[] = KNOWN;		% jansen/jansen_linkage.m
INTIAL_ESTIMATE[] = KNOWN;	% 

FOR CRANK_ANGLE in CRANK_ANGLES:									%
	LINK_ANGLES = NEWTON_RAPHSON(INITIAL_ESTIMATE, CRANK_ANGLE);	%	jansen/nr.m
		XN+1 = XN - INV(DF)/F;										%	jansen/matrix_invert.m
	INITIAL_ESTIMATE = LINK_ANGLES;									%

LINK_COORDS = LINK_POSITIONS(LINK_ANGLES, LINK LENGTHS);			% 	jansen/get_coords.m

ANIMATE_LINKAGE(FRAME);												%	jansen/animate.m
